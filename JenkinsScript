pipeline {
    agent any
    stages{
        
        stage ('Clone and clean repository'){
            steps{
                script{
                    bat "git clone https://github.com/Alaeddinehnana/Bootstrap4"
                    bat "mvn clean -f Bootstrap4"
                }
            }
        }
        
        stage('Test'){
            steps{
                bat "mvn test -f Bootstrap4"
            }
        }
        
        }
        }
